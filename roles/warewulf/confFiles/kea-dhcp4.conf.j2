{
  "Dhcp4": {
    "interfaces-config": {
      "interfaces": [ "enp2s0" ]
    },
    "control-socket": {
      "socket-type": "unix",
      "socket-name": "/tmp/kea4-ctrl-socket"
    },
    "lease-database": {
      "type": "memfile",
      "lfc-interval": 3600
    },
    "renew-timer": 900,
    "rebind-timer": 1800,
    "valid-lifetime": 3600,
    "subnet4": [
      {
        "id": 1,
        "subnet": "192.168.1.0/24",
        "valid-lifetime": 18000,
        "pools": [ { "pool": "192.168.1.100 - 192.168.1.200" } ],
        "option-data": [
          {
            "name": "routers",
            "data": "192.168.1.1"
          },
          {
            "name": "domain-name-servers",
            "data": "192.168.1.2, 192.168.1.3"
          },
          {
            "name": "domain-name",
            "data": "grup1.itb.cat"
          }
        ]
      },
      {
        "id": 2,
        "subnet": "172.16.1.0/24",
        "valid-lifetime": 32400,
        "pools": [ { "pool": "172.16.1.10 - 172.16.1.100" } ],
        "option-data": [
          {
            "name": "routers",
            "data": "172.16.1.1"
          },
          {
            "name": "domain-name-servers",
            "data": "192.168.1.2, 192.168.1.3"
          },
          {
            "name": "domain-name",
            "data": "grup1.itb.cat"
          }
        ]
      }
    ],
    "reservations": [
      {
        "hw-address": "52:54:00:49:24:9C",
        "ip-address": "172.16.1.250"
      },
      {
        "hw-address": "52:54:00:1C:C8:48",
        "ip-address": "172.16.1.150"
      }
    ]
  },
  "loggers": [
    {
      "name": "kea-dhcp4",
      "output_options": [
        {
          "output": "/var/log/kea-dhcp4.log"
        }
      ]
    }
  ],
  "severity": "INFO",
  "debuglevel": 0
}
